{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "xvrlang",
      "camelcase": "XvrLang",
      "title": "xvr lang",
      "scope": "source.xvrlang",
      "file-types": [
        "xvr"
      ],
      "injection-regex": "^xvrlang$",
      "class-name": "TreeSitterXvrlang"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree sitter for xvr language",
    "authors": [
      {
        "name": "arfy slowy",
        "email": "slowy.arfy@proton.me",
        "url": "https://github.com/slowy07"
      }
    ],
    "links": {
      "repository": "https://github.com/WargaSlowy/xvrlang-treesitter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}